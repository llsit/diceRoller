pipeline {
    agent any
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello world!' 
            }
        }
        stage("SonarQube analysis") {
            steps {
              withSonarQubeEnv('SonarQubeServer') {
                    bat 'start cmd.exe /c C:\\sonar-scanner-cli\\bin\\sonar-scanner.bat'
              }
            }
          }
    }

}